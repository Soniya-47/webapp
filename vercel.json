{
    "version": 2,
    "functions": {
        "backend/api/index.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "backend/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "backend/api/index.ts"
        }
    ]
}